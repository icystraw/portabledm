<Window x:Class="partialdownloadgui.YTDownload"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:partialdownloadgui"
        mc:Ignorable="d"
        Title="Add Youtube Download" Width="600" WindowStartupLocation="CenterOwner" Background="#FFF2F5FC" ShowInTaskbar="False" SizeToContent="Height" ResizeMode="NoResize">
    <StackPanel>
        <TextBox Name="txtUrl"></TextBox>
        <StackPanel Name="spChkContainer"></StackPanel>
        <Button Name="btnOK" Click="btnOK_Click">Get Videos</Button>
        <Button Name="btnDownload" IsEnabled="False" Click="btnDownload_Click">Download Selected Videos</Button>
    </StackPanel>
</Window>
