{
  "name": "Link Grabber",
  "version": "1.0",
  "description": "Take over download from browser",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "downloads",
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "16": "16.png",
      "48": "48.png",
      "128": "128.png"
    }
  }
}