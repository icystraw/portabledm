{
  "name": "Link Grabber for Portable HTTP Download Manager",
  "version": "1.0",
  "description": "Take over downloads from Chromium based browser",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "downloads",
    "storage"
  ],
  "action": {
    "default_icon": {
      "16": "16.png",
      "48": "48.png",
      "128": "128.png"
    }
  }
}