{
  "name": "Link Grabber",
  "version": "1.0",
  "description": "Take over download from browser",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [ "downloads", "storage", "tabs" ],
  "host_permissions": [ "http://localhost:13000/" ],
  "action": {
    "default_icon": {
      "16": "16.png",
      "48": "48.png",
      "128": "128.png"
    }
  }
}
